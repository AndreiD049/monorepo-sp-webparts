{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "cip-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/cip/CipWebPart.js",
          "manifest": "./src/webparts/cip/CipWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "sp-preset": {
      "path": "https://cdn.jsdelivr.net/npm/sp-preset@2.0.21/dist/index.amd.js",
      "globalName": "spPreset"
    },
    "sp-react-notifications": {
      "path": "https://cdn.jsdelivr.net/npm/sp-react-notifications@1.0.23/dist/index.amd.js",
      "globalName": "spReactNotifications"
    },
    "luxon": {
      "path": "https://cdn.jsdelivr.net/npm/luxon@2.3.1/build/global/luxon.min.js",
      "globalName": "luxon"
    }
  },
  "localizedResources": {
    "CipWebPartStrings": "lib/webparts/cip/loc/{locale}.js"
  }
}
